{
    "name": "csci432-final-project",
    "private": true,
    "version": "0.0.2",
    "type": "module",
    "scripts": {
        "dev": "npx tsx server.ts",
        "build": "tsc -b && vite build",
        "preview": "vite preview",
        "lint": "eslint . --report-unused-disable-directives --fix && tsc --noEmit --emitDeclarationOnly false",
        "prettier": "prettier ./ --write --ignore-path ./node_modules/phantom-config/.prettierignore",
        "stylelint": "npx stylelint --ignore-path .gitignore '**/*.{css,scss}' --fix",
        "format": "npm run lint && npm run stylelint && npm run prettier",
        "prepare": "husky install"
    },
    "dependencies": {
        "express": "^4.21.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.2",
        "vite-express": "^0.19.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "globals": "^15.9.0",
        "husky": "^8.0.0",
        "lint-staged": "^15.2.10",
        "phantom-config": "^0.5.2",
        "sass": "^1.78.0",
        "tsx": "^4.19.1",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1"
    }
}
